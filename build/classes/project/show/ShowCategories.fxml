<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="background" prefHeight="600.0" prefWidth="800.0" stylesheets="@main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.show.ShowCategoriesController">
   <padding>
      <Insets bottom="10.0" />
   </padding>
   <center>
      <TableView fx:id="table" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="id" editable="false" prefWidth="75.0" visible="false" />
          <TableColumn fx:id="cat" editable="false" maxWidth="1.7976931348623157E308" prefWidth="599.0" text="Dostępne kategorie" />
        </columns>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <bottom>
      <HBox>
         <children>
            <Button mnemonicParsing="false" onAction="#delete" text="Usuń" BorderPane.alignment="CENTER" />
            <Button mnemonicParsing="false" onAction="#add_cat" text="Dodaj" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
